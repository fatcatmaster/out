    {
      "name": "spring.mybatis.url",
      "type": "java.lang.String",
      "description": "数据库服务器地址",
      "sourceType": "com.fatcat.mybatis.config.MybatisValueConfiguration"
    },
    {
      "name": "spring.mybatis.use-unicode",
      "type": "java.lang.Boolean",
      "description": "使用自定义编码为 utf-8;默认gbk",
      "sourceType": "com.fatcat.mybatis.config.MybatisValueConfiguration",
      "defaultValue": true
    }
  ],
